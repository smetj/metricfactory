{
  "system":{
    "metrics":false,
    "metrics_interval":10,
    "metrics_dst":"logging"
  },
  "bootstrap":{
    "udpsocket":{
      "module":"wishbone.io_modules.udpsocket",
      "class":"UDPSocket",
      "variables":{
        "port":8649
      }
    },
    "ganglia":{
      "module":"decoders.decodeganglia",
      "class":"DecodeGanglia",
      "variables":{

      }
    },
    "stdout":{
      "module":"wishbone.modules.stdout",
      "class":"STDOUT",
      "variables":{
        "purge":true
      }
    }
  },
  "routingtable":{
    "udpsocket.inbox":[
      "ganglia.inbox"
    ],
    "ganglia.outbox":[
      "stdout.inbox"
    ]
  }
}
